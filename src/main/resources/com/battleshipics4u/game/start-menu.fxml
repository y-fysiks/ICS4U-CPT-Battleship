<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="720.0" prefWidth="1280.0" spacing="20.0" style="-fx-background-color: #000000;" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.battleshipics4u.game.menuControllers.StartController">
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
  </padding>
  <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
    <Image url="@battleship_logo.png"/>
  </ImageView>



  <Button onAction="#onStartButtonClick" prefHeight="68.0" prefWidth="200.0" style="-fx-background-color: #000000; -fx-border-color: #008000;" text="Start" textFill="#00ff2f">
    <font>
      <Font name="Agency FB Bold" size="36.0" />
    </font>
    <effect>
      <Glow level="0.99" />
    </effect></Button>
  <Button onAction="#onHelpButtonClick" prefHeight="51.0" prefWidth="200.0" style="-fx-background-color: #000000; -fx-border-color: #008000;" text="Instructions" textFill="#00ff1e">
    <font>
      <Font name="Agency FB Bold" size="28.0" />
    </font>
    <effect>
      <Glow level="1.0" />
    </effect></Button>
  <Button layoutX="550.0" layoutY="387.0" onAction="#onExitButtonClick" prefHeight="45.0" prefWidth="96.0" style="-fx-background-color: #000000; -fx-border-color: #008000;" text="Exit" textFill="#04ff00">
    <font>
      <Font name="Agency FB Bold" size="24.0" />
    </font>
    <effect>
      <Glow level="1.0" />
    </effect>
  </Button>
</VBox>